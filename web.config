<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="next">
          <match url="/*" />
          <action type="Rewrite" url="server.js" />
        </rule>
      </rules>
    </rewrite>
    <iisnode watchedFiles="web.config;*.js;iisnode-interceptor.js" nodeProcessCommandLine="&quot;%programfiles%\nodejs\node.exe&quot;" loggingEnabled="true" devErrorsEnabled="true" interceptor="iisnode-interceptor.js" />
  </system.webServer>
</configuration>
